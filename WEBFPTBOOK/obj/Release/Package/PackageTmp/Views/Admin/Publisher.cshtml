@model IEnumerable<WEBFPTBOOK.Models.Publisher>

@{
    ViewBag.Title = "Publisher";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Publisher</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PubID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PubName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PubID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PubName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PubID })
            @Html.ActionLink("Details", "Details", new { id=item.PubID }) 
            @Html.ActionLink("Delete", "Delete", new { id=item.PubID })
        </td>
    </tr>
}

</table>
